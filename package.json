{
  "name": "zobtrade",
  "version": "1.0.0",
  "description": "ZobTrade - B2B Fuel & Petroleum Supplier from Georgia",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "build:css": "tailwindcss -i ./src/assets/css/input.css -o ./public/css/styles.css --minify",
    "watch:css": "tailwindcss -i ./src/assets/css/input.css -o ./public/css/styles.css --watch",
    "db:migrate": "node database/migrate.js",
    "db:seed": "node database/seed.js",
    "generate:icons": "node scripts/generate-icons.js"
  },
  "keywords": [
    "petroleum",
    "fuel",
    "supplier",
    "georgia",
    "central-asia",
    "b2b",
    "trading"
  ],
  "author": "ZobTrade",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "connect-pg-simple": "^9.0.1",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "i18n": "^0.15.1",
    "node-telegram-bot-api": "^0.64.0",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sharp": "^0.34.5",
    "tailwindcss": "^3.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
